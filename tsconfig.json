{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],

  "compilerOptions": {
    "noEmit": false,
    "checkJs": false,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },

  "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
  "include": ["src"],
  "exclude": ["node_modules"]
}
